            "input": 0.000004,
            "output": 0.000016
          },
          "options": {
            "enable_thinking": true
          }
        },
        "qwen-coder-plus": {
          "name": "Qwen Coder Plus",
          "limit": { "context": 1000000, "output": 65536 },
          "cost": {
            "input": 0.000006,
            "output": 0.000024
          },
          "modalities": { "input": ["text"], "output": ["text"] }
        },
        "qwen-coder-flash": {
          "name": "Qwen Coder Flash",
          "limit": { "context": 1000000, "output": 65536 },
          "modalities": { "input": ["text"], "output": ["text"] }
